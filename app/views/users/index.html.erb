<div class="row" style="margin-top:40px;">
<div class="col-md-4 col-md-offset-4 col-xs-12  white-scroller" >
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <thead>
          <tr>
            <th>Email</th>
            <th>Login</th>
            <th>Role</th>
            <th></th>
            <th></th>
          </tr>
        </thead>        
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
</div>
</div>