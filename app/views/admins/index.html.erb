  <div class="row" style="margin-top:40px;">
    <div class="col-md-1">
    &nbsp;
    </div>  
    <div class="col-md-10  white-scroller" >

<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="responsive-table" style="margin:auto;width:100%;">
  <thead>
    <tr>
      <th>Email</th>
      <th>Login</th>
      <th>Role</th>
      <th>Sign in count</th>
      <th>Current sign in at</th>
      <th>Last sign in at</th>
      <th>Current sign in ip</th>
      <th>Last sign in ip</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"><%= link_to new_admin_path do %><i aria-hidden="false" class="fa fa-plus"></i><% end %></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.email %></td>
        <td><%= admin.login %></td>
        <td><%= admin.role %></td>
        <td><%= admin.sign_in_count %></td>
        <td><%= admin.current_sign_in_at.strftime("%Y-%m-%d %R") %></td>
        <td><%= admin.last_sign_in_at.strftime("%Y-%m-%d %R") %></td>
        <td><%= admin.current_sign_in_ip %></td>
        <td><%= admin.last_sign_in_ip %></td>
        <td><%= admin.created_at.strftime("%Y-%m-%d %R") %></td>
        <td><%= admin.updated_at.strftime("%Y-%m-%d %R") %></td>
        <td style="padding-left:1rem;"><%= link_to admin do %><i aria-hidden="false" class="fa fa-eye"></i><% end %></td>
        <td style="padding-left:1rem;"><%= link_to edit_admin_path(admin) do %><i aria-hidden="false" class="fa fa-pencil"></i><% end %></td>
        <td style="padding-left:1rem;"><%= link_to admin, method: :delete, data: { confirm: 'Are you sure?' } do %><i aria-hidden="false" class="fa fa-trash"></i><% end %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



</div>
</div>